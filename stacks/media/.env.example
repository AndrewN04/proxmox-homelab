TZ=America/Chicago
PUID=1000
PGID=1000

# Gluetun (Proton WireGuard) - fill from Proton config
VPN_SERVICE_PROVIDER=protonvpn
VPN_TYPE=wireguard
VPN_PORT_FORWARDING=on
WIREGUARD_PRIVATE_KEY=<PROTON_WG_PRIVATE_KEY>
WIREGUARD_ADDRESSES=<PROTON_WG_ADDRESSES>
SERVER_COUNTRIES=<REGION_COUNTRY_CODE_OR_NAME>

# qB WebUI
QBIT_WEBUI_PORT=8080
QBIT_USER=admin
QBIT_PASS=<SET_STRONG_PASSWORD>

# qSticky (must match config.toml)
GLUETUN_AUTH_TYPE=apikey
GLUETUN_APIKEY=<RANDOM_LONG_API_KEY>
